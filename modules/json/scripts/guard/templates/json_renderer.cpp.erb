#include <crails/renderers/json_renderer.hpp>

using namespace Crails;
using namespace std;

<% templates.each do |template| -%>
string <%= template[:function] %>(SharedVars&);
<% end -%>

JsonRenderer::JsonRenderer()
{
<% templates.each do |template| -%>
  templates.insert(
    pair<string, Generator>(<%= template[:name].inspect %>, <%= template[:function] %>)
  );
<% end -%>
}
