#include <crails/renderers/html_renderer.hpp>

using namespace Crails;
using namespace std;

<% templates.each do |template| -%>
string <%= template[:function] %>(const Renderer*, SharedVars&);
<% end -%>

HtmlRenderer::HtmlRenderer()
{
<% templates.each do |template| -%>
  templates.insert(
    pair<string, Generator>(<%= template[:name].inspect %>, <%= template[:function] %>)
  );
<% end -%>
}
